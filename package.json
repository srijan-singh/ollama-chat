{
    "name": "ollama-chat",
    "displayName": "Ollama Chat",
    "description": "A VS Code extension to chat with a locally running Ollama model",
    "version": "1.0.0",
    "publisher": "Srijan Singh",
    "engines": {
        "vscode": "^1.75.0"
    },
    "license": "MIT",
    "categories": [
        "Chat",
        "Extension Packs"
    ],
    "activationEvents": [
        "onCommand:ollama-chat.start"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "ollama-chat.start",
                "title": "Start Ollama Chat"
            }
        ]
    },
    "dependencies": {
        "axios": "^1.6.0",
        "marked": "^11.1.0",
        "dompurify": "^3.2.4",
        "highlight.js": "^11.8.0"
    },
    "devDependencies": {
        "@types/vscode": "^1.75.0",
        "typescript": "^5.8.2"
    },
    "scripts": {
        "prepublish": "echo 'No compile needed'",
        "compile": "echo 'Skipping TypeScript'"
    }
}